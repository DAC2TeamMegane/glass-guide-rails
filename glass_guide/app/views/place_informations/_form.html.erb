<%= form_for(@place_information) do |f| %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </div>
  <div class="field">
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </div>
  <div class="field">
    <%= f.label :icon_url %><br />
    <%= f.text_field :icon_url %>
  </div>
  <div class="field">
    <%= f.label :beacon_data1 %><br />
    <%= f.number_field :beacon_data1 %>
  </div>
  <div class="field">
    <%= f.label :beacon_data2 %><br />
    <%= f.number_field :beacon_data2 %>
  </div>
  <div class="actions">
    <% if f.object.new_record? %>
      <%= f.submit "追加",class: "btn btn-default" %>
    <% else %>
      <%= f.submit "更新",class: "btn btn-default" %>
    <% end %>
  </div>
<% end %>
